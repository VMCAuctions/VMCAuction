
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<!-- Bootstrap -->
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.6.3/css/all.css" integrity="sha384-UHRtZLI+pbxtHCWp1t77Bi1L4ZtiqrqD80Kn4Z8NTSRyMA2Fd33n5dQ8lWUE00s/" crossorigin="anonymous">
<link rel="stylesheet" type="text/css" href="/css/style.css" >
<link rel="stylesheet" type="text/css" href="/css/adminHeader.css">

<!-- JavaScript-->
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

<header>
	<!-- Organizer Top Navbar -->
	<nav>
		<div id="top-nav" class="d-flex flex-row bg-light justify-content-between align-items-center py-3">
			<div class="col-5">
				<div class="brand">elizabid</div>
		    </div>	

			<!-- Displaying AUCTION NAME in the center of the top navigation bar-->	
			<div class="col-2 text-center">
			    <a href="/<%=auction._id%>/event" class="auction-nav"><%= auctionDetails.name %></a> 
			</div> 

			<div class="col-5 navbar navbar-expand-sm flex-row justify-content-end align-items-center">
				<!-- "current" is a flag sent from the server to indicate the active web page -->
				<ul class="navbar-nav">
					<li>
						<a class="mr-4 top-link  <% if (typeof(current) != 'undefined' && current == 'dashboard') { %> active1 <% } %>" href="/auctions/main">My Dashboard</a>
					</li>
					<li>
						<a class="mr-4 top-link <% if (typeof(current) != 'undefined' && current == 'organizerMenu') { %> active1 <% } %>" href="/<%=auction._id%>/organizerMenu">Auction Menu</a>
					</li>
		
					<li>
						<a class="mr-4 top-link <% if (typeof(current) != 'undefined' && current == 'myAccount') { %> active1 <% } %>" href="/<%=auction._id%>/users/account/<%=userName%>"> <i class="fas fa-user"></i> My Account</a>
					</li>
					<li>
						<a class="mr-4" href="/users/logout"><i class="fas fa-sign-out-alt"></i> Logout</a>
					</li>
				</ul>
			</div>
		</div>
    </nav> 	
	
	<!-- Organizer Secondary Navbar-->
  	<nav id="second-nav" class="navbar navbar-expand-md navbar-dark pl-4">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
          	<span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarCollapse">
			<!-- "current" is a flag sent from the server to indicate the active web page -->
          	<ul class="navbar-nav">
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'addItem') { %> active2 <% } %>" href="/<%=auction._id%>/items/new"> Add Item</a>
				</li>	
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'createPackage') { %> active2 <% } %>" href="/<%=auction._id%>/packages/new">Create Package</a>
				</li>
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'item-register') { %> active2 <% } %>" href="/<%=auction._id%>/items">Item Register</a>
				</li>
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'package-register') { %> active2 <% } %>" href="/<%=auction._id%>/packages/list">Package Register </a>
				</li>
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'catalog') { %> active2 <% } %>" href="/<%=auction._id%>/packages">Catalog</a>
				</li>
				<li>
					<a class="mr-4 nav-link second-link <% if (typeof(current) != 'undefined' && current == 'supporters') { %> active2 <% } %>" href='/<%=auction._id%>/users'>Supporter Register</a>
				</li>
          	</ul>
        </div>
    </nav>
</header>
