							<a class="mr-2 top-link" href="/<%=auction._id%>/clerkDash">Clerk Dashboard</a>
						</div>
						<div>
							<a class="mr-2" href="/users/logout"><i class="fas fa-sign-out-alt"></i> Logout</a>
						</div>
					<!-- </ul> -->
				<!-- </div> -->
		</div>
    </nav>
